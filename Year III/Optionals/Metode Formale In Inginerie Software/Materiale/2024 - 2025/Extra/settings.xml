<?xml version="1.0" encoding="UTF-8"?>
<settings xmlns="http://maven.apache.org/SETTINGS/1.1.0"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/SETTINGS/1.1.0 http://maven.apache.org/xsd/settings-1.1.0.xsd">


   <profiles>
      <profile>
         <id>jsxm</id>
         <activation>
            <activeByDefault>true</activeByDefault>
         </activation>
         <repositories>
            <repository>
               <id>archetype</id>
               <url>http://jsxm.org/maven2/</url>
               <releases>
                  <enabled>true</enabled>
                  <checksumPolicy>fail</checksumPolicy>
               </releases>
               <snapshots>
                  <enabled>true</enabled>
                  <checksumPolicy>warn</checksumPolicy>
               </snapshots>
            </repository>
         </repositories>
         <pluginRepositories>
            <pluginRepository>
               <id>central</id>
               <name>Central Repository</name>
               <url>https://repo.maven.apache.org/maven2</url>
               <layout>default</layout>
               <snapshots>
                  <enabled>false</enabled>
               </snapshots>
               <releases>
                  <updatePolicy>never</updatePolicy>
               </releases>
            </pluginRepository>
            <pluginRepository>
               <releases>
                  <enabled>true</enabled>
                  <updatePolicy>always</updatePolicy>
                  <checksumPolicy>fail</checksumPolicy>
               </releases>
               <id>jsxm</id>
               <name>JSXM Model Based Testing</name>
               <url>http://jsxm.org/maven2/</url>
            </pluginRepository>
         </pluginRepositories>
      </profile>
   </profiles>
</settings>
